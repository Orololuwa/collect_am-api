create_table("users"){
    t.Column("id", "integer", {primary: true})
    t.Column("email", "string", {})
    t.Column("first_name", "string", {})
    t.Column("last_name", "string", {})
    t.Column("phone", "string", {})
    t.Column("password", "string", {size: 60})
